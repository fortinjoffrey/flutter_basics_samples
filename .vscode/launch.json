  "version": "0.2.0",
  "configurations": [
    {
      "name": "app (debug)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        "api_keys.json",
        "--dart-define-from-file",
        "common_env.json",
      ]
    },
    {
      "name": "app (profile)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file", "api_keys.json", "--dart-define=APP_NAME=Location Profile"]
    },
    {
      "name": "app (release)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file", "api_keys.json", "--dart-define=APP_NAME=Location Release"]
    }
  ]
}
